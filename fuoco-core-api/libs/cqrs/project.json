{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/cqrs/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/cqrs",
        "main": "libs/cqrs/src/index.ts",
        "tsConfig": "libs/cqrs/tsconfig.lib.json",
        "webpackConfig": "libs/cqrs/webpack.config.js"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": []
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/cqrs/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cqrs"],
      "options": {
        "jestConfig": "libs/cqrs/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
